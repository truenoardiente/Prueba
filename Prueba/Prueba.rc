        SS_LEFT | WS_CHILD | WS_VISIBLE,
        8, 9, 84, 8
    CONTROL "Aceptar", IDOK, "button",
    BS_PUSHBUTTON | BS_CENTER | WS_CHILD | WS_VISIBLE | WS_TABSTOP,
    56, 26, 50, 14
END
